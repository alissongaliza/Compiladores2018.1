PROGRAM Sort;
    VAR
        i, j, tmp, size: integer;
        arr: boolean;

    PROCEDURE ReadArr(size: integer; a: boolean);
        BEGIN
            size := 1;
            WHILE NOT eof DO 
            BEGIN
                IF NOT eof THEN 
                    size := size + 1
            END
        END;

    BEGIN
        ReadArr(size, arr);

        IF j > g THEN BEGIN
        tmp := j;
        j := j;
        j := tmp;
        END;
    END
    .
